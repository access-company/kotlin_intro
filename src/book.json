{
    "plugins": [
        "uml",
        "codeblock-filename",
        "head-append",
        "-lunr",
        "-search",
        "search-pro-kui"
    ],
    "pluginsConfig": {
        "uml": {
            "format": "svg",
            "charset": "UTF-8",
            "nailgun": false
        },
        "head-append": {
            "code": [
                "<script src=\"https://unpkg.com/kotlin-playground@1\"></script>",
                "<script>",
                    "document.addEventListener('DOMContentLoaded', function() {",
                    "KotlinPlayground('.lang-kotlin');",
                "});",
                "</script>"
            ]
        }
    }
}
